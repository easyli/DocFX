{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "getting-started/**.md",
          "getting-started/**/toc.yml",
          "toc.yml",
          "*.md",
          "api/**.md",
          "api/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          ".attachments/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "../material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© easyli</span>",
      "_appTitle": "DocFX",
      "_enableSearch": "true",
      "_disableContribution": "true",
      "_gitContribute": {
        "repo": "https://github.com/easyli/DocFX"
      },
      "_gitUrlPattern": "github"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  },
  "sitemap":
  {
    "baseUrl": "https://easyli.github.io/DocFX"
  }
}
